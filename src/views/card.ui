<interface>
  <object id="card-grid" class="GtkGrid">
    <property name ="visible">True</property>
    <child>
      <object id="heading-lbl" class="GtkLabel">
        <property name="visible">True</property>
      </object>
      <packing>
        <property name="left-attach">0</property>
        <property name="top-attach">0</property>
      </packing>
    </child>
    <child>
      <object id="content-lbl" class="GtkLabel">
        <property name="visible">True</property>
      </object>
      <packing>
        <property name="left-attach">0</property>
        <property name="top-attach">1</property>
      </packing>
    </child>
  </object>
</interface>

